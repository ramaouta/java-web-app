pipeline {
  agent any //{ label 'linux' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sq1') { 
           sh 'mvn clean test sonar:sonar -Dsonar.host.url=http://192.168.240.138:9000'
        }
      }
    }
  }
}
